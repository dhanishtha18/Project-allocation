<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegisterStudentActivity"
    android:background="#e1f5fe"
    android:orientation="vertical">
    <include
        android:id="@+id/bar"
        layout="@layout/toolbar"/>

    <androidx.appcompat.widget.AppCompatTextView
        style="@style/textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="*Note: All Field's of Student Details are Mandatory. Else Student will not be Registered."
        android:textColor="#FF0000"/>

    <androidx.appcompat.widget.AppCompatSpinner
        style="@style/spinner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/select_semester"/>


    <androidx.appcompat.widget.AppCompatButton
        style="@style/btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/upload_document"
        android:text="Select File and Register Students" />

</androidx.appcompat.widget.LinearLayoutCompat>